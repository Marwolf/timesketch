# Ignore frontend build related files
node_modules
timesketch/static/dist

# Ignore back-up files.
*~

# Ignore compiled Python files.
*.pyc
*.pyo

# Don't include build related files.
/dependencies/
/dist/
/build/
*.egg-info

# And don't care about the 'egg'.
/Timesketch.egg-info

# Test files
.coverage
tests-coverage.txt

# Exclude Vagrant runtime files
vagrant/.vagrant/
vagrant/*.log

\.DS_Store

# Docker related volumes
docker/lib/elasticsearch/*
!docker/lib/elasticsearch/.gitsave
docker/lib/postgresql/data/*
!docker/lib/postgresql/data/.gitsave
docker/lib/neo4j/data/*
!docker/lib/neo4j/data/.gitsave
docker/lib/redis/*
!docker/lib/redis/.gitsave
